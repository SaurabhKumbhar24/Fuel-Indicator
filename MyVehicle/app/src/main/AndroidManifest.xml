<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin.myvehicle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".List" />
        <activity android:name=".Milege" />
        <activity android:name=".volume" />
        <activity android:name=".Distance" />
        <activity android:name=".speed" />
        <activity android:name=".locate" />
        <activity android:name=".WaterLevel" />
        <activity android:name=".history"></activity>
    </application>

</manifest>